{
  "projectType": "fullstack",
  "frontend": {
    "framework": "react",
    "buildTool": "umijs",
    "uiLibrary": "ant.design",
    "uiVersion": "4.24.8",
    "preferredLanguage": "typescript"
  },
  "backend": {
    "language": "python",
    "preferredFrameworks": ["fastapi", "flask", "django"],
    "startCommand": "uvicorn main:app --host 0.0.0.0 --port 8000",
    "dependencies": ["fastapi", "uvicorn", "python-multipart", "pillow", "sqlalchemy"],
    "paths": {
      "static": "../frontend",
      "data": "../data",
      "assets": "../frontend/assets"
    },
    "environment": "auto_ai_subtitle-v0.0.9"
  },
  "database": {
    "type": "PostgreSQL",
    "orm": "sqlalchemy"
  },
  "styling": {
    "cssPreprocessor": "less",
    "preferredApproach": "css-in-js"
  },
  "packageManager": "npm",
  "conventions": {
    "componentStyle": "functional",
    "pythonStyle": "pep8",
    "imports": {
      "react": "import React from 'react'",
      "antd": "import { Component } from 'antd'",
      "umi": "import { useModel } from 'umi'"
    },
    "changelog": {
      "path": "docs/changes",
      "format": "{000}-{commit}.md",
      "template": {
        "title": "# [{number}] {title}",
        "sections": [
          "Date: YYYY-MM-DD",
          "## Changes",
          "## Related Files Changed",
          "## Dependencies Updated"
        ]
      }
    }
  },
  "testing": {
    "frontend": "@umijs/test",
    "backend": "pytest"
  },
  "dev": {
    "port": 8000,
    "proxy": {
      "/api": "http://localhost:8000"
    }
  },
  "environment": {
    "node": "16.20.2",
    "npm": ">=6.0.0"
  }
} 